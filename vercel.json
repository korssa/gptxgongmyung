{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "STORAGE_TYPE": "vercel-blob",
    "NEXT_PUBLIC_STORAGE_TYPE": "vercel-blob"
  }
}
